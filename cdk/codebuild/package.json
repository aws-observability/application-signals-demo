{
  "name": "application-signals-codebuild",
  "version": "1.0.0",
  "description": "AWS CodeBuild stack for building Application Signals demo Docker images",
  "bin": {
    "codebuild": "bin/codebuild.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy",
    "destroy": "cdk destroy",
    "synth": "npm run build && cdk synth",
    "upload-source": "npm run build && node ./scripts/upload-source.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.8.10",
    "aws-cdk": "^2.1025.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.208.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
